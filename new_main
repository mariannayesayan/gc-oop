Class Student(User):

   def __init__ (self, firstname, lastname, ID, password, username, mail, courses, assignment, gpa):
       self.courses = courses
       self.assignment = assignment
       self.gpa = gpa
       user.init(self, firstname, lastname, ID, password, username, mail):


   def set courses(self, courses):
       self.courses = courses
       return self.courses


   def set assignment(self, assignment):
       self.assignment = assignment
       return self.assignment


   def set gpa(self, gpa):
       self.gpa = gpa
       return self.gpa


     def get courses(self, courses):
         self.courses = courses
         return self.courses


     def get assignment(self, assignment):
         self.assignment = assignment
         return self.assignment


     def get gpa(self, gpa):
         self.gpa = gpa
         return self.gpa


Class lecturer (User):

   def __init__ (self, firstname, lastname, ID, password, username, mail, office):
       self.office = office
       user.init(self, firstname, lastname, ID, password, username, mail)


   def set office(self, office):
       self.office = office
       return self.office

   def get office (self, office):
        self.office = office
        return self. office


class Grade:
    min = 0
    max = 100

    def __init__(self, grade):
        self.grade = grade

    def _get_(self):
            return self.grade

    def _set_(self, grade):
        self.grade = grade
        return self.grade

    def displayGrade (self) :
        print " the grade is" , self.grade

    def is_Valid (self) :
        if self.grade <= self.max  and self.grade > self.min:
            print "Not valid"
        else:
            print "Not valid"


