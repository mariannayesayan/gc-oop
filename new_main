Class user:

   def __init__ (self, firstname, lastname, ID, password, username, mail):
      self.firstname = firstname
      self.lastname = lastname
      self.ID = ID
      self.password = password
      self.username = username
      self.mail = mail


   def set firstname(self, firstname)
   if firstname != "":
      self.firtname = firstname
      return self.firstname


   def set lastname(self, lastname)
   if lastname != "":
      self.lastname = lastname
    return self.lasttname


   def set ID(self, ID)
