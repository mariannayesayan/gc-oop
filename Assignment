class Assignment:
    min = 0
    max = 100

    def __init__(self, name, percentage, grade, deadline, status, desciprion):
        self.name = name
        self.percentage = percentage
        self.grade = grade
        self.deadline = deadline
        self.status = status
        self.description = description

    def _get_(self, name):
            return self.name

    def _set_(self, name):
            return self.name

    def _set_(self, percantage):
            return self.percenateg

    def _set_(self, grade):
            return self.grade

    def _set_(self, deadline):
            return self.deadline

    def _set_(self, status):
            return self.status

    def _set_(self, description):
            return self.description

    def is_Late (self):
        return self
    def is_Graded (self):
        return self






